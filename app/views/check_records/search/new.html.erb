<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <h1 class="govuk-heading-l">Find a teacher record</h1>

    <%= form_with url: check_records_result_path, method: :get do |f| %>
      <%= f.govuk_text_field :trn, class: "govuk-input--width-20", label: { text: "Teacher reference number (TRN)" } %>
      <%= f.govuk_submit "Search" %>
    <% end %>
  </div>
</div>


