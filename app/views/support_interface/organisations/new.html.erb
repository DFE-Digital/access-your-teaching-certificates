<% content_for :page_title, 'Organisations' %>

<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <h1 class="govuk-heading-l">New organisation</h1>

    <%= form_with model: @organisation, url: support_interface_organisations_path do |f| %>
      <%= f.govuk_text_field :company_registration_number, label: { text: 'Company registration number' } %>

      <%= f.govuk_submit 'Create organisation' %>
    <% end %>
  </div>
</div>