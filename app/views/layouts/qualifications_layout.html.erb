<% content_for :head do %>
  <%= tag :meta, name: 'robots', content: 'noindex' %>
<% end %>

<% content_for :navigation do %>
  <%= render(Qualifications::NavigationComponent.new(current_user: current_user)) %>
<% end %>

<% content_for :content do %>
  <%= yield %>
<% end %>

<%= render template: 'layouts/base' %>
